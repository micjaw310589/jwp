<Window x:Class="lab4.DaneWejscioweOkno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4"
        mc:Ignorable="d"
        Title="DaneWejscioweOkno" Height="412" Width="692" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,0,-6">
        <Label x:Name="labelSzerokosc" Content="Szerokość" HorizontalAlignment="Left" Margin="149,111,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelWysokosc" Content="Wysokość" HorizontalAlignment="Left" Margin="149,142,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtSzerokosc" HorizontalAlignment="Left" Margin="280,115,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" x:FieldModifier="private"/>
        <TextBox x:Name="txtWysokosc" HorizontalAlignment="Left" Margin="280,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" x:FieldModifier="private"/>
        <Button x:Name="btnOk" Content="Ok" HorizontalAlignment="Left" Margin="235,233,0,0" VerticalAlignment="Top" Height="63" Width="122" Click="btnOk_Click"/>

    </Grid>
</Window>
