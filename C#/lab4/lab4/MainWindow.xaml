<Window x:Class="lab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnOblicz" Content="Oblicz" HorizontalAlignment="Left" Margin="400,336,0,0" VerticalAlignment="Top" Height="62" Width="128" Click="btnOblicz_Click"/>
        <Button x:Name="btnObliczKule" Content="Oblicz kule" HorizontalAlignment="Left" Margin="272,255,0,0" VerticalAlignment="Top" Height="62" Width="128" Click="btnObliczKule_Click"/>
        <Button x:Name="btnObliczStozek" Content="Oblicz stożek" HorizontalAlignment="Left" Margin="528,255,0,0" VerticalAlignment="Top" Height="62" Width="128" Click="btnObliczStozek_Click"/>
        <Label x:Name="labelKula" Content="Tu pojawi się kula" HorizontalAlignment="Left" Margin="272,38,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelStozek" Content="Tu pojawi się stożek" HorizontalAlignment="Left" Margin="528,38,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnWyswietl" Content="Wyświetl" HorizontalAlignment="Left" Margin="43,20,0,0" VerticalAlignment="Top" Height="62" Width="128" Click="btnWyswietl_Click"/>
        <ListBox x:Name="listWyswietl" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="24,101,598,117"/>

    </Grid>
</Window>
