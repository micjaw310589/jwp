<Window x:Class="zad3_lab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zad3_lab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listPojazdy" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="btnWypisz" Grid.Column="1" Content="Wypisz" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top" Height="60" Width="136" Click="btnWypisz_Click"/>

    </Grid>
</Window>
